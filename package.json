{
  "name": "authserver",
  "version": "1.0.0",
  "description": "Authentication & Authorization dedicated server.",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node ./dist/server.js",
    "dev-ts": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/server.ts",
    "mock-prod": "npm run build && export NODE_ENV=production && npm run start",
    "prod": "npm run build && npm run start"
  },
  "author": "okarim4259",
  "license": "MIT",
  "dependencies": {
    "app-root-path": "^2.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "inversify": "^5.0.1",
    "inversify-express-utils": "^6.2.0",
    "log4js": "^3.0.6",
    "passport": "^0.4.0",
    "passport-google-plus-token": "^2.1.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.7.1",
    "reflect-metadata": "^0.1.12",
    "typeorm": "^0.2.9"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.16.0",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/node": "^10.12.12",
    "@types/passport": "^0.4.7",
    "@types/passport-jwt": "^3.0.1",
    "typescript": "^3.2.1"
  }
}
